    <sql>DROP TABLE IF EXISTS dim_date_created;

CREATE TABLE dim_date_created (
  id int(18) unsigned NOT NULL AUTO_INCREMENT,
  year smallint(5) unsigned NOT NULL,
  month tinyint(2) unsigned NOT NULL,
  day tinyint(2) unsigned NOT NULL,
  hour tinyint(2) unsigned NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;

INSERT INTO dim_date_created
	(SELECT DISTINCT NULL, YEAR(i.date_created) year, MONTH(i.date_created) month, DAY(i.date_created) day, HOUR(i.date_created) hour
		FROM (
  			SELECT DISTINCT date_created FROM issue where active = true
		) AS i);</sql>
