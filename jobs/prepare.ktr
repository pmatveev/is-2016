    <sql>DROP TABLE IF EXISTS dim_assignee;

CREATE TABLE dim_assignee (
  id int(18) unsigned NOT NULL AUTO_INCREMENT,
  idt int(18) unsigned NOT NULL,
  group_name varchar(32) NOT NULL,
  name varchar(32) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;

INSERT INTO dim_assignee
	(SELECT DISTINCT NULL, idt, asg.group_name, asg.name
		FROM (
  			SELECT DISTINCT a.id idt, g.name group_name, a.credentials name
			FROM issue i, officer a, officer_group g 
			where i.active = true
			and a.id = i.assignee
			and g.id = a.officer_group__id
		) AS asg);</sql>
