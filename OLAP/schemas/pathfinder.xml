<Schema name="Pathfinder">
  <Dimension type="TimeDimension" visible="true" foreignKey="CREATED" highCardinality="false" name="Time created">
    <Hierarchy name="Time hierarchy" visible="true" hasAll="true">
      <Table name="dim_date_created">
      </Table>
      <Level name="Year" visible="true" table="dim_date_created" column="id" type="String" uniqueMembers="false" levelType="TimeYears" hideMemberIf="Never" caption="Year">
        <KeyExpression>
          <SQL dialect="generic">
            <![CDATA[year]]>
          </SQL>
        </KeyExpression>
      </Level>
      <Level name="Month" visible="true" table="dim_date_created" column="id" type="String" uniqueMembers="false" levelType="TimeMonths" hideMemberIf="Never" caption="Month">
        <KeyExpression>
          <SQL dialect="generic">
            <![CDATA[CONCAT(year, ', ', MONTHNAME(STR_TO_DATE(month, '%m'))]]>
          </SQL>
        </KeyExpression>
      </Level>
      <Level name="Day" visible="true" table="dim_date_created" column="id" type="String" uniqueMembers="false" levelType="TimeDays" hideMemberIf="Never" caption="Day">
        <KeyExpression>
          <SQL dialect="generic">
            <![CDATA[CONCAT(LPAD(day, 2, 0), '.', LPAD(month, 2, 0), '.', year)]]>
          </SQL>
        </KeyExpression>
      </Level>
      <Level name="Hour" visible="true" table="dim_date_created" column="id" type="String" uniqueMembers="false" levelType="TimeHours" hideMemberIf="Never" caption="Hour">
        <KeyExpression>
          <SQL dialect="generic">
            <![CDATA[CONCAT(LPAD(day, 2, 0), '.', LPAD(month, 2, 0), '.', year, ' ', hour, ':00-', hour, ':59')]]>
          </SQL>
        </KeyExpression>
      </Level>
    </Hierarchy>
  </Dimension>
  <Cube name="issue" caption="Issue" visible="true" cache="true" enabled="true">
    <Table name="fact_issue">
    </Table>
    <DimensionUsage source="Time created" name="Time created" visible="true" foreignKey="CREATED" highCardinality="false">
    </DimensionUsage>
    <Measure name="IssueCount" column="ID" datatype="Numeric" aggregator="distinct-count" caption="Count" visible="true">
    </Measure>
  </Cube>
</Schema>
